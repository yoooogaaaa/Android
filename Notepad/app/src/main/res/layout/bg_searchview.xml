<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <corners android:radius="35dp"/>
    <!-- 连框颜色值 -->
    <item>
        <shape>
            <solid android:color="@color/floralwhite" />
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item
        android:top="1dp"
        android:left="1dp"
        android:right="1dp">

        <shape>
            <corners android:radius="35dp"/>

            <solid android:color="@color/antiquewhite" />
        </shape>
    </item>
</layer-list>